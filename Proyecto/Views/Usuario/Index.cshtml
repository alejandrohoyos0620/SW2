@model IEnumerable<Proyecto.Models.UsuarioModel>
@{
    ViewBag.Title = "Usuario";
}

<h2>Usuario</h2>
<p>
    @Html.ActionLink("Crear nuevo usuario", "Create")
</p>

<table class="table">
    <tr>
        <th>
            identificacion
        </th>
        <th>
            Nombre
        </th>
        <th>
            Contraseña
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id_usuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contraseña)
            </td>           
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id_usuario }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id_usuario }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id_usuario })
            </td>
        </tr>
    }

</table>

